                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH InvHist AS
(
SELECT       DISTINCT h.InvoiceNo, h.HeaderSeqNo, d.DetailSeqNo, ItemCode, ItemCodeDesc, QuantityShipped, UnitPrice, ExtensionAmt,ItemType
FROM            MAS_POL.dbo.AR_InvoiceHistoryHeader h INNER JOIN
				MAS_POL.dbo.AR_InvoiceHistoryDetail d on h.InvoiceNo = d.InvoiceNo and h.HeaderSeqNo = d.HeaderSeqNo
WHERE ModuleCode = 'S/O' and InvoiceDate &gt;= DATEADD(YEAR, - 2, GETDATE()) and ItemCode NOT IN ('/C','/COBRA')
UNION ALL
SELECT        DISTINCT h.InvoiceNo, '0' as HeaderSeqNo, d.LineKey AS DetailSeqNo, ItemCode, ItemCodeDesc, QuantityShipped, UnitPrice, ExtensionAmt,ItemType
FROM            MAS_POL.dbo.SO_InvoiceHeader h INNER JOIN
				MAS_POL.dbo.SO_InvoiceDetail d on h.InvoiceNo = d.InvoiceNo
WHERE ItemCode NOT IN ('/C','/COBRA')
)
SELECT 
	h.InvoiceNo as 'InvNo'
	, CAST(h.HeaderSeqNo AS INT) AS HeaderSeqNo
	, CAST(h.DetailSeqNo AS INT) AS DetailSeqNo
	, CASE WHEN h.ItemType = '1' THEN '' ELSE h.ItemCodeDesc END AS 'Desc'
	, h.ItemCode
	, CONVERT(DECIMAL(9,4),(ROUND(QuantityShipped,4))) AS Qty
	, CONVERT(DECIMAL(9,2),(ROUND(UnitPrice,2))) AS Pri
	, CONVERT(DECIMAL(9,2),(ROUND(ExtensionAmt,2))) AS Tot
FROM InvHist h
ORDER BY InvoiceNo, CAST(h.HeaderSeqNo AS INT), CAST(h.DetailSeqNo AS INT)</property>
