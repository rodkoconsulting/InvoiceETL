            <component
              refId="Package\Data Flow Task\Invoices Source"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="Invoices Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH InvHist AS
(
SELECT       h.InvoiceNo,h.HeaderSeqNo, h.InvoiceDate, h.InvoiceType, h.ARDivisionNo, h.CustomerNo, h.SalespersonNo, h.Comment, h.UDF_NJ_COOP, DetailSeqNo, ItemCode, QuantityShipped, UnitPrice, ExtensionAmt
FROM            MAS_POL.dbo.AR_InvoiceHistoryHeader h INNER JOIN
				MAS_POL.dbo.AR_InvoiceHistoryDetail d on h.InvoiceNo = d.InvoiceNo and h.HeaderSeqNo = d.HeaderSeqNo
WHERE ModuleCode = 'S/O' and InvoiceDate &gt;= DATEADD(YEAR, - 2, GETDATE()) and ItemCode NOT IN ('/C','/COBRA')
UNION ALL
SELECT        h.InvoiceNo, '000000' as HeaderSeqNo, h.InvoiceDate, h.InvoiceType, h.ARDivisionNo, h.CustomerNo, h.SalespersonNo, h.Comment, h.UDF_NJ_COOP, LineKey as DetailSeqNo, ItemCode, QuantityShipped, UnitPrice, ExtensionAmt
FROM            MAS_POL.dbo.SO_InvoiceHeader h INNER JOIN
				MAS_POL.dbo.SO_InvoiceDetail d on h.InvoiceNo = d.InvoiceNo
WHERE ItemCode NOT IN ('/C','/COBRA')
)
SELECT 
	h.InvoiceNo as 'InvNo'
,h.HeaderSeqNo
    , CASE WHEN h.InvoiceType = 'CM' THEN 'C' ELSE 'I' END AS Typ
	, h.ARDivisionNo + h.CustomerNo AS AcctNo
	, h.InvoiceDate as Date
	, h.Comment as Cmt
	, h.UDF_NJ_COOP AS Coop
	, h.SalespersonNo AS InvR
	, h.ItemCode
                  , DetailSeqNo
	, ROUND(QuantityShipped,2) AS Qty
	, ROUND(UnitPrice,2) AS Pri
	, ROUND(ExtensionAmt,2) AS Tot
FROM InvHist h</property>
